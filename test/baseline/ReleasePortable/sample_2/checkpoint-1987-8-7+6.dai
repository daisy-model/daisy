;; Create a checkpoint of the entire simulation state, suitable for later
;; hot start.
;; Input files.
(input file "tillage.dai")
(input file "crop.dai")
(input file "log.dai")
;; From file '/home/silas/Projects/daisy-model/daisy/test/dai_test_files/sample-2.dai':
(defhorizon Ap FAO3
  (clay 0.08 [])
  (silt 0.105 [])
  (sand 0.815 [])
  (humus 0.0112 [])
  (description "Andeby top soil.")
  (C_per_N 11 [g C/g N])
  (dry_bulk_density 1.5 [g/cm^3]))

(defhorizon C Ap
  (humus 0.0012 [])
  (description "Andeby C horizon."))

(defcolumn Andeby default
  (Soil (horizons (-20 [cm]
                   (Ap))
                  (-250 [cm]
                   (C)))
        (MaxRootingDepth 60 [cm])
        (border -100 [cm]))
  (description "Data collected by F.Guf at the B.And farm, Andeby, 2002.")
  (Groundwater deep)
  (OrganicMatter original (init (root 480 [kg C/ha/y])
                                (end -20 [cm])
                                (input 1400 [kg C/ha/y]))))

(defprogram AndebyFarm Daisy
  (stop 1988 4 1 1)
  (weather default "dk-taastrup.dwf")
  (time 1986 12 1 1)
  (column Andeby)
  (output harvest
          ("Field nitrogen" (when monthly))
          ("Soil nitrogen" (from 0 [cm])
                           (to -100 [cm])
                           (when daily))
          ("Field water" (when monthly))
          ("Soil water" (from 0 [cm])
                        (to -100 [cm])
                        (when daily))
          (Crop (crop "Spring Barley")
                (where "sbarley.dlf"))
          (checkpoint (when at (1987 8 7 6))))
  (manager activity (wait (at (1987 3 20 1)))
                    plowing
                    (wait (at (1987 4 4 1)))
                    (fertilize (mineral (weight 100 [kg N/ha])
                                        (NH4_fraction 0.5 [])))
                    (wait (at (1987 4 5 1)))
                    (progn (sow (Grass))
                           (sow ("Spring Barley")))
                    (wait (or (crop_ds_after "Spring Barley" 2 [])
                              (at (1987 9 5 1))))
                    (harvest "Spring Barley")
                    (wait (at (1987 9 8 1)))
                    (fertilize (mineral (weight 80 [kg N/ha])
                                        (NH4_fraction 0.5 [])))
                    (wait (at (1987 10 10 1)))
                    (cut Grass
                         (stub 8 [cm]))))
